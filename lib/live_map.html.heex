<svg
  version="1.1"
  xmlns="http://www.w3.org/2000/svg"
  width={@width} height={@height}
>
  <title><%= @title %></title>

  <svg>
    <%= for tile <- @tiles do %>
      <image href={"https://tile.openstreetmap.org/#{@zoom}/#{tile.x}/#{tile.y}.png"} x="0" y="0" width="256px" height="256px" />
    <% end %>
  </svg>

  <style></style>
</svg>
